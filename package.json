{
  "name": "gacha",
  "version": "0.0.7",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "webpack --config webpack.main.prod.config.js",
    "build-renderer": "webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "dev-renderer": "cross-env APP_ENV=dev webpack-dev-server --config webpack.renderer.dev.config.js",
    "dev-main": "cross-env APP_ENV=dev webpack --config webpack.main.config.js && cross-env APP_ENV=dev electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 APP_ENV=dev npm run dev-renderer",
    "start": "electron .",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "test": "jest --config jest.config.json --collectCoverage --passWithNoTests",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "rm -rf release dist && npm run build && electron-builder",
    "prod": "cross-env DEBUG=off && npm run dist",
    "preview": "cross-env DEBUG=on && npm run dist",
    "postinstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{json,css,scss,md}": [
      "prettier --config ./.prettierrc --write"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "eslint --ext=jsx,js,ts,tsx --fix src"
    ]
  },
  "build": {
    "productName": "GACha",
    "appId": "org.ppzoho.gacha",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.education"
    },
    "linux": {
      "category": "Science",
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zohoMatt/gacha.git"
  },
  "author": {
    "name": "MattZuo",
    "email": "mattzuo12@gmail.com"
  },
  "license": "GNU V3",
  "bugs": {
    "url": "https://github.com/zohoMatt/gacha/issues"
  },
  "homepage": "https://github.com/zohoMatt/gacha",
  "devDependencies": {
